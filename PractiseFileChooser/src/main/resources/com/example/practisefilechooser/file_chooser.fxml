<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<AnchorPane fx:id="ap"
            xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.practisefilechooser.FileChooser1"
            prefHeight="400.0" prefWidth="600.0">

    <children>
        <ImageView fx:id="imageView" fitHeight="224.0" fitWidth="396.0" layoutX="100.0" layoutY="50.0"
                   pickOnBounds="true" preserveRatio="true"/>
        <Button layoutX="256.0" layoutY="300.0" mnemonicParsing="false" onAction="#chooseImage" text="Open"/>
    </children>

</AnchorPane>
